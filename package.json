{
  "name": "pong",
  "private": true,
  "version": "0.1.0",
  "description": "Demo multi-player pong on Cloud Run with MongoDB",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc && webpack",
    "watch": "chokidar \"src/client/**/*\" \"src/lib\" \"src/server/**/*\" -c \"npm run build\"",
    "predev": "npm run build",
    "dev": "NODE_ENV=development concurrently \"npm run watch\" \"nodemon -q --trace-warnings dist/server/main.js\"",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node dist/server/main.js",
    "test": "echo 'No tests' && exit 0"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "protobufjs": "^7.0.0",
    "uuid": "^8.3.2",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.6",
    "@types/p5": "^1.4.2",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "del": "^6.1.1",
    "node-polyfill-webpack-plugin": "^2.0.0",
    "nodemon": "^2.0.19",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
